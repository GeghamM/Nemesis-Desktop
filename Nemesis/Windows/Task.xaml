<Window x:Class="Nemesis.Task"
        x:Name="TaskScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nemesis"
        WindowState="Maximized"
        SizeChanged="WindowSozeChanged"
        StateChanged="WindwStateChanged"
        KeyDown="TaskKeyDown"
        Title="ԽՆԴԻՐ" Height="730" MinHeight="400" Width="1005" MinWidth="1005">
    <Grid x:Name="MainGrid" Style="{DynamicResource Window-main-grid}" MouseUp="ExitExpanders">
        <Expander Style="{DynamicResource ExpanderStyle}" x:Name="ToolBoxExpander" Panel.ZIndex="50" LostFocus="ExitExpander" ExpandDirection="Left" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,8,7,0" ScrollViewer.VerticalScrollBarVisibility="Disabled">
            <Border Style="{DynamicResource Border-Main}">
                <StackPanel Style="{DynamicResource StackPanel}" x:Name="ToolBoxPanel" HorizontalAlignment="Center"  VerticalAlignment="Center" >
                    <StackPanel HorizontalAlignment="Center" Margin="0,2" MouseUp="ResetClick" Cursor="Hand">
                        <StackPanel.RenderTransform>
                            <ScaleTransform x:Name="ScaleTransform1" ScaleX="1" ScaleY="1" CenterX="15" CenterY="15"/>
                        </StackPanel.RenderTransform>
                        <StackPanel.Triggers>
                            <EventTrigger RoutedEvent="MouseEnter">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform1" Storyboard.TargetProperty="ScaleX"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform1" Storyboard.TargetProperty="ScaleY"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="MouseLeave">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform1" Storyboard.TargetProperty="ScaleX"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform1" Storyboard.TargetProperty="ScaleY"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </StackPanel.Triggers>
                        <Image x:Name="Clear" Margin="10,2" Height="20"/>
                        <Label Style="{DynamicResource Label_Style}" FontSize="9" Content="Մաքրել"/>
                    </StackPanel>
                    <StackPanel HorizontalAlignment="Center" Margin="0,2" MouseUp="SaveTarget" Cursor="Hand">
                        <StackPanel.RenderTransform>
                            <ScaleTransform x:Name="ScaleTransform2" ScaleX="1" ScaleY="1" CenterX="25" CenterY="25"/>
                        </StackPanel.RenderTransform>
                        <StackPanel.Triggers>
                            <EventTrigger RoutedEvent="MouseEnter">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform2" Storyboard.TargetProperty="ScaleX"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform2" Storyboard.TargetProperty="ScaleY"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="MouseLeave">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform2" Storyboard.TargetProperty="ScaleX"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform2" Storyboard.TargetProperty="ScaleY"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </StackPanel.Triggers>
                        <Image x:Name="Plus" Margin="10,2" Height="20"/>
                        <Label Style="{DynamicResource Label_Style}" FontSize="9" Content="Ավելացնել"/>
                    </StackPanel>
                    <StackPanel HorizontalAlignment="Center" Margin="0,2" MouseUp="SaveScreen" Cursor="Hand">
                        <StackPanel.RenderTransform>
                            <ScaleTransform x:Name="ScaleTransform3" ScaleX="1" ScaleY="1" CenterX="25" CenterY="25"/>
                        </StackPanel.RenderTransform>
                        <StackPanel.Triggers>
                            <EventTrigger RoutedEvent="MouseEnter">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform3" Storyboard.TargetProperty="ScaleX"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform3" Storyboard.TargetProperty="ScaleY"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="MouseLeave">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform3" Storyboard.TargetProperty="ScaleX"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform3" Storyboard.TargetProperty="ScaleY"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </StackPanel.Triggers>
                        <Image x:Name="ScreenShot" Height="30" VerticalAlignment="Center" Margin="10,-2.5"/>
                        <Label Style="{DynamicResource Label_Style}" Margin="0,0,0,0" FontSize="9" Content="Պահպանել"/>
                    </StackPanel>
                    <StackPanel HorizontalAlignment="Center" Margin="0,2" MouseUp="ShowSob" Cursor="Hand">
                        <StackPanel.RenderTransform>
                            <ScaleTransform x:Name="ScaleTransform4" ScaleX="1" ScaleY="1" CenterX="25" CenterY="25"/>
                        </StackPanel.RenderTransform>
                        <StackPanel.Triggers>
                            <EventTrigger RoutedEvent="MouseEnter">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform4" Storyboard.TargetProperty="ScaleX"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform4" Storyboard.TargetProperty="ScaleY"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="MouseLeave">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform4" Storyboard.TargetProperty="ScaleX"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform4" Storyboard.TargetProperty="ScaleY"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </StackPanel.Triggers>
                        <Image x:Name="SobNote" Height="30" VerticalAlignment="Center" Margin="10,-2.5"/>
                        <Label Style="{DynamicResource Label_Style}" Margin="0,0,0,0" FontSize="9" Content="ՄԱՍ-ի բլոկնոտ"/>
                    </StackPanel>
                    <StackPanel HorizontalAlignment="Center" Margin="0,2" MouseUp="ShowHranot" Cursor="Hand">
                        <StackPanel.RenderTransform>
                            <ScaleTransform x:Name="ScaleTransform5" ScaleX="1" ScaleY="1" CenterX="25" CenterY="25"/>
                        </StackPanel.RenderTransform>
                        <StackPanel.Triggers>
                            <EventTrigger RoutedEvent="MouseEnter">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform5" Storyboard.TargetProperty="ScaleX"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform5" Storyboard.TargetProperty="ScaleY"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="MouseLeave">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform5" Storyboard.TargetProperty="ScaleX"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform5" Storyboard.TargetProperty="ScaleY"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </StackPanel.Triggers>
                        <Image x:Name="HranotNote" Height="30" VerticalAlignment="Center" Margin="10,-2.5"/>
                        <Label Style="{DynamicResource Label_Style}" Margin="0,0,0,0" FontSize="9" Content="ՀՀ-ի բլոկնոտ"/>
                    </StackPanel>
                    <StackPanel HorizontalAlignment="Center" Margin="0,2" MouseUp="ShowDitarkum" Cursor="Hand">
                        <StackPanel.RenderTransform>
                            <ScaleTransform x:Name="ScaleTransform6" ScaleX="1" ScaleY="1" CenterX="25" CenterY="25"/>
                        </StackPanel.RenderTransform>
                        <StackPanel.Triggers>
                            <EventTrigger RoutedEvent="MouseEnter">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform6" Storyboard.TargetProperty="ScaleX"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform6" Storyboard.TargetProperty="ScaleY"
                                                         From="1" To="1.22" Duration="0:0:0.2" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                            <EventTrigger RoutedEvent="MouseLeave">
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform6" Storyboard.TargetProperty="ScaleX"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                        <DoubleAnimation Storyboard.TargetName="ScaleTransform6" Storyboard.TargetProperty="ScaleY"
                                                         From="1.22" To="1" Duration="0:0:0.3" AutoReverse="False"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </EventTrigger>
                        </StackPanel.Triggers>
                        <Image x:Name="DitarkumImage" Height="30" VerticalAlignment="Center" Margin="10,-2.5"/>
                        <Label Style="{DynamicResource Label_Style}" Margin="0,0,0,0" FontSize="9" Content="Դիտարկում"/>
                    </StackPanel>
                </StackPanel>
            </Border>
        </Expander>
        <Expander Style="{DynamicResource ExpanderStyle}" x:Name="KayficentPanel" Visibility="Visible" Panel.ZIndex="50" ExpandDirection="Left" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,40,7,0" ScrollViewer.VerticalScrollBarVisibility="Disabled">
            <Border Style="{DynamicResource Border-Main}">
                <StackPanel Style="{DynamicResource StackPanel}">
                    <DockPanel>
                        <Label Visibility="Hidden" Style="{DynamicResource Label_Style}" Margin="15,10,5,0"  Width="50" Height="22">Д հենք</Label>
                        <Grid Margin="0,10,15,0">
                            <TextBox Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" FontSize="13" Padding="2,6,2,1" x:Name="K_Distance" Width="70"></TextBox>
                            <Label Style="{DynamicResource LabelBack_Style}" FontSize="13" Padding="5,0,5,0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,-10" >K</Label>
                        </Grid>
                        <Grid Margin="0,10,5,0">
                            <TextBox Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" FontSize="13" Padding="2,6,2,1" x:Name="K_Davarot" Width="70"></TextBox>
                            <Label Style="{DynamicResource LabelBack_Style}" FontSize="13" Padding="5,0,3,0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,-10" >Δ∂Rնփ</Label>
                        </Grid>
                        <Grid Margin="0,10,5,0">
                            <TextBox Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" FontSize="13" Padding="2,6,2,1" x:Name="Z_Reper" Width="70"></TextBox>
                            <Label Style="{DynamicResource LabelBack_Style}" FontSize="13" Padding="5,0,5,0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15,-10" >ZR</Label>
                        </Grid>
                    </DockPanel>
                    <StackPanel Margin="0,5,0,10" HorizontalAlignment="Center">
                        <DockPanel>
                            <Label Style="{DynamicResource Label_Style}" FontWeight="ExtraBold" Margin="15,10,5,0" HorizontalContentAlignment="Right" Width="50" Padding="0" Height="22" VerticalContentAlignment="Center">Д հենք</Label>
                            <TextBox x:Name="K_Dist1" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            <TextBox x:Name="K_Dist2" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            <TextBox x:Name="K_Dist3" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                        </DockPanel>
                        <DockPanel>
                            <Label Style="{DynamicResource Label_Style}" FontWeight="ExtraBold" Margin="15,10,5,0" HorizontalContentAlignment="Right" Padding="0" Height="22" VerticalContentAlignment="Center" Width="50">ΔД գում․</Label>
                            <TextBox x:Name="K_DeltaDistance1" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            <TextBox x:Name="K_DeltaDistance2" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            <TextBox x:Name="K_DeltaDistance3" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                        </DockPanel>
                        <DockPanel>
                            <Label Style="{DynamicResource Label_Style}" FontWeight="ExtraBold" Margin="15,10,5,0" HorizontalContentAlignment="Right" Padding="0" Height="22" VerticalContentAlignment="Center"  Width="50">Δ∂ գում</Label>
                            <TextBox x:Name="K_DeltaDavarot1" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            <TextBox x:Name="K_DeltaDavarot2" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            <TextBox x:Name="K_DeltaDavarot3" Style="{DynamicResource TextBox_Style}" TextChanged="KayficentChanged" Width="70" Margin="0,10,15,0" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                        </DockPanel>
                    </StackPanel>
                </StackPanel>
            </Border>
        </Expander>
        <StackPanel>
            <StackPanel HorizontalAlignment="Center" >
                <StackPanel>
                    <WrapPanel Orientation="Horizontal" x:Name="MainToolbarStackPanel" Margin="5,5,30,5"  HorizontalAlignment="Center">
                        <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" HorizontalAlignment="Center" x:Name="Systems"  Margin="3" DropDownClosed="SystemChanged" FontSize="11.5" FontWeight="ExtraBold" Width="100"/>
                        <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" HorizontalAlignment="Center" x:Name="Positions"  Margin="3" DropDownClosed="Count" SelectionChanged="HandleView" FontSize="11.5" FontWeight="ExtraBold" Width="140"/>
                        <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" HorizontalAlignment="Center" x:Name="TaskType"  Margin="3" DropDownClosed="TaskTypechanged" FontSize="11.5" FontWeight="ExtraBold" Width="210"/>
                        <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" HorizontalAlignment="Center" x:Name="ShootingCore" Margin="3" DropDownClosed="Count" FontSize="11.5" FontWeight="ExtraBold" Width="120"/>
                        <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" HorizontalAlignment="Center" x:Name="Paytucich"  Margin="3" DropDownClosed="PaytucichClosed" FontSize="11.5" FontWeight="ExtraBold" Width="120"/>
                        <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" HorizontalAlignment="Center" x:Name="ShootingMethod" DropDownClosed="Count"  Margin="3" FontSize="11.5" FontWeight="ExtraBold" Width="105"/>
                        <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" HorizontalAlignment="Center" x:Name="IsKayficent" SelectedIndex="0" Margin="3" DropDownClosed="KayficentDropdownClosed" FontSize="11.5" FontWeight="ExtraBold" Width="110" >
                            <ComboBoxItem>Տեղագրական</ComboBoxItem>
                            <ComboBoxItem>Հաշվարկային</ComboBoxItem>
                        </ComboBox>
                    </WrapPanel>
                    <StackPanel x:Name="AXK_xyh" Visibility="Collapsed" Orientation="Horizontal" HorizontalAlignment="Center">
                        <StackPanel x:Name="AXKNamePanel" VerticalAlignment="Center"  HorizontalAlignment="Center">
                            <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14" FontWeight="ExtraBold" HorizontalAlignment="Center">ԱՆՈՒՆԸ</Label>
                            <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" x:Name="AXKName"  Margin="2,0" SelectionChanged="AXKName_SelectionChanged" DropDownClosed="AXKNameSelected" FontSize="12"  FontWeight="ExtraBold" Width="140"></ComboBox>
                        </StackPanel>
                        <StackPanel x:Name="TexamasPanel" VerticalAlignment="Center"  HorizontalAlignment="Center">
                            <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14" FontWeight="ExtraBold" HorizontalAlignment="Center">ԿՈՈՐԴԻՆԱՏՆԵՐԸ</Label>
                            <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" x:Name="AXKType"  Margin="2,0" DropDownClosed="TaskTypechanged" FontSize="12"  FontWeight="ExtraBold" Width="140"></ComboBox>
                        </StackPanel>
                        <StackPanel x:Name="AXKTypePanel" VerticalAlignment="Center" HorizontalAlignment="Center">
                            <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14" FontWeight="ExtraBold" HorizontalAlignment="Center">ՏԵՂԱՄԱՍ</Label>
                            <ComboBox Template="{DynamicResource ComboBox}" Style="{DynamicResource ComboBox_Style}" x:Name="Texamas"  Margin="2,0" DropDownClosed="Texamas_DropDownClosed"  FontSize="12"  FontWeight="ExtraBold" Width="140"></ComboBox>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="10,0,10,0">
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">X ձախ.</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}"  MaxLength="7" x:Name="AXK_dzax_X" Margin="2,0" FontSize="14" Keyboard.KeyUp="AXKFokusLost"  Width="75" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">Y ձախ.</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}" MaxLength="7" x:Name="AXK_dzax_Y" Margin="2,0" FontSize="14" Keyboard.KeyUp="AXKFokusLost"  Width="75" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">H ձախ.</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}" MaxLength="4" x:Name="AXK_dzax_H" FontSize="14" Keyboard.KeyUp="AXKFokusLost"  Width="55" FontWeight="ExtraBold" HorizontalContentAlignment="Center"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="10,0,10,0">
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14" FontWeight="ExtraBold" HorizontalAlignment="Center">X աջ.</Label>
                                <TextBox  Style="{DynamicResource TextBox_Style}" MaxLength="7" x:Name="AXK_aj_X" Margin="2,0" FontSize="14" Keyboard.KeyUp="AXKFokusLost" Width="75" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14" FontWeight="ExtraBold" HorizontalAlignment="Center">Y աջ.</Label>
                                <TextBox  Style="{DynamicResource TextBox_Style}" MaxLength="7" x:Name="AXK_aj_Y" Margin="2,0" FontSize="14" Keyboard.KeyUp="AXKFokusLost" Width="75" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">H աջ.</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}" MaxLength="4" x:Name="AXK_aj_H" FontSize="14" Keyboard.KeyUp="AXKFokusLost"  Width="55" FontWeight="ExtraBold" HorizontalContentAlignment="Center"/>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                    <WrapPanel Orientation="Horizontal" x:Name="Planayin" HorizontalAlignment="Center">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">ՆՇ․ N</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}" MaxLength="7" x:Name="TargetNumber" TextChanged="OnTargetNumberChanged" Margin="2,0" FontSize="14"  Width="90" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">ԱՆՈՒՆԸ / ԲՆՈՒՅԹԸ</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}" MaxLength="50" x:Name="TargetName" Margin="2,0" FontSize="14"  Width="302" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">X</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}"  MaxLength="7" x:Name="Target_X" Margin="2,0" FontSize="14" LostFocus="TargetLostFocus"  Width="75" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">Y</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}" MaxLength="7" x:Name="Target_Y" Margin="2,0" FontSize="14" LostFocus="TargetLostFocus"  Width="75" FontWeight="ExtraBold" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                            </StackPanel>
                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center"  Margin="5,0">
                                <Label Style="{DynamicResource Label_Style}" Margin="2,0" FontSize="14"  FontWeight="ExtraBold" HorizontalAlignment="Center">H</Label>
                                <TextBox Style="{DynamicResource TextBox_Style}" MaxLength="4" x:Name="Target_H" FontSize="14" LostFocus="TargetLostFocus"  Width="55" FontWeight="ExtraBold" HorizontalContentAlignment="Center"/>
                            </StackPanel>
